{
  "name": "react-sass-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass": "sass --watch src/styles/sass:src/styles/css",
    "sass:build": "sass - update src/styles/sass:src/styles/css"
  },
  "devDependencies": {
    "node-sass": "^4.9.3"
  }
}
